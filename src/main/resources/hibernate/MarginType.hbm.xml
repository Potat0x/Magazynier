<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="magazynier">

    <class name="magazynier.MarginType" table="margin_types">
        <id name="id" column="type_id" type="int">
            <generator class="org.hibernate.id.enhanced.SequenceStyleGenerator">
                <param name="sequence_name">MARGIN_TYPES_TYPE_ID_SEQ</param>
            </generator>
        </id>

        <property name="typeName" column="type_name"/>
    </class>

</hibernate-mapping>