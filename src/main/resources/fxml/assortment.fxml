<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<VBox prefHeight="471.0" prefWidth="558.0" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="magazynier.AssortmentController">
    <children>
        <HBox prefHeight="2.0" prefWidth="666.0">
            <children>
                <Button contentDisplay="TOP" mnemonicParsing="false" onAction="#addItem" prefWidth="105.0"
                        text="Nowy towar">
                    <graphic>
                        <ImageView>
                            <image>
                                <Image url="@../img/document-new.png"/>
                            </image>
                        </ImageView>
                    </graphic>
                </Button>
                <Button fx:id="editButton" contentDisplay="TOP" disable="true" mnemonicParsing="false"
                        onAction="#editItem" prefWidth="105.0" text="Edytuj">
                    <graphic>
                        <ImageView>
                            <image>
                                <Image url="@../img/format-justify-fill.png"/>
                            </image>
                        </ImageView>
                    </graphic>
                </Button>
                <Button fx:id="deleteButton" contentDisplay="TOP" disable="true" mnemonicParsing="false"
                        onAction="#deleteItem" prefWidth="105.0" text="Usuń">
                    <graphic>
                        <ImageView>
                            <image>
                                <Image url="@../img/edit-delete.png"/>
                            </image>
                        </ImageView>
                    </graphic>
                </Button>
                <GridPane prefHeight="61.0" prefWidth="237.0">
                    <columnConstraints>
                        <ColumnConstraints hgrow="SOMETIMES" maxWidth="217.0" minWidth="10.0" prefWidth="137.0"/>
                        <ColumnConstraints hgrow="SOMETIMES" maxWidth="142.0" minWidth="10.0" prefWidth="133.0"/>
                    </columnConstraints>
                    <rowConstraints>
                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                    </rowConstraints>
                    <children>
                        <Label text="Wartość netto"/>
                        <Label text="Wartość brutto" GridPane.rowIndex="1"/>
                        <Label alignment="CENTER_RIGHT" contentDisplay="RIGHT" prefHeight="17.0" prefWidth="107.0"
                               text="12334,tttt43 zł" GridPane.columnIndex="1" GridPane.halignment="RIGHT"
                               GridPane.hgrow="ALWAYS"/>
                        <Label alignment="CENTER_RIGHT" contentDisplay="RIGHT" prefHeight="17.0" prefWidth="110.0"
                               text="12 zł" GridPane.columnIndex="1" GridPane.halignment="RIGHT" GridPane.hgrow="ALWAYS"
                               GridPane.rowIndex="1"/>
                    </children>
                    <padding>
                        <Insets bottom="8.0" left="8.0" right="8.0" top="8.0"/>
                    </padding>
                </GridPane>
            </children>
        </HBox>
        <HBox prefHeight="601.0" prefWidth="760.0">
            <children>
                <TableView fx:id="itemsTable" prefHeight="601.0" prefWidth="124.0" tableMenuButtonVisible="true"
                           HBox.hgrow="ALWAYS">
                    <columns>
                        <TableColumn fx:id="nameCol" prefWidth="75.0" text="Nazwa"/>
                        <TableColumn prefWidth="75.0" text="Partia(ukryte)" visible="false"/>
                        <TableColumn prefWidth="66.0" text="ID(ukryte)" visible="false"/>
                        <TableColumn fx:id="eanCol" prefWidth="76.0" text="EAN"/>
                        <TableColumn fx:id="itemModelNumberCol" prefWidth="97.0" text="Kod producenta"/>
                        <TableColumn fx:id="quantityCol" prefWidth="52.0" text="Stan"/>
                        <TableColumn fx:id="priceCol" prefWidth="67.0" text="Cena"/>
                        <TableColumn fx:id="taxCol" prefWidth="75.0" text="Podatek"/>
                        <TableColumn prefWidth="77.0" text="Dostawca(ukr)" visible="false"/>
                        <TableColumn prefWidth="67.9752197265625" text="Typ marży(ukr)" visible="false"/>
                        <TableColumn prefWidth="80.727294921875" text="Marża(ukr)" visible="false"/>
                        <TableColumn fx:id="warehousesCol" prefWidth="75.0" text="Magazyny"/>
                    </columns>
                    <columnResizePolicy>
                        <TableView fx:constant="CONSTRAINED_RESIZE_POLICY"/>
                    </columnResizePolicy>
                </TableView>
            </children>
        </HBox>
    </children>
</VBox>
